class pila:
    def_init__(self):
        self.items=[]
    def encolar(self,x):
        self.items.append(x)
    def desencolar(self):
        try:
            return.self.items.pop(0)
        except:
            raise ValueError("la cola esta vacia")
        def es__vacia(self):
            return self.items==[]
        def mostrar_pila(self):
            print()
        
                
